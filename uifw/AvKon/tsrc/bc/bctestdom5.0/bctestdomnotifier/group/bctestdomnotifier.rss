RESOURCE FORM r_number_field_form
    {
    items =
        {
        DLG_LINE
            {
            type = EEikCtEdwin;
            prompt = qtn_bctestdomnotifier_outline1;
            id = EBCTestCmdOutline1;
            itemflags = EEikDlgItemTakesEnterKey | EEikDlgItemOfferAllHotKeys;
            control = EDWIN
                {
                flags = EEikEdwinNoHorizScrolling | EEikEdwinResizable;
                width = qtn_width;
                lines = qtn_line;
                maxlength = qtn_maxlength;
                max_view_height_in_lines = qtn_view_line; 
                base_line_delta = qtn_base_line; 
                };
            tooltip = qtn_bctestdomnotifier_outline1;
            }
        };
    }

