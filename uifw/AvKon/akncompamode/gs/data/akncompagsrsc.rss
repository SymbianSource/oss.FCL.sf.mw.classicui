RESOURCE MENU_BAR r_gs_menubar_change_exit
    {
    titles =
        {
        MENU_TITLE
            {
            menu_pane = r_gs_menu_item_exit;
            },
        MENU_TITLE
            {
            menu_pane = r_gs_menu_item_help;
            },
        MENU_TITLE
            {
            menu_pane = r_gs_menu_item_change;
            }
        };
